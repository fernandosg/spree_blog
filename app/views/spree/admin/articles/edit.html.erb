<h1>Editing article</h1>
<ul class="nav nav-tabs" role="tablist">
  <li class="nav-item">
    <a class="nav-link active" id="form-tab" data-toggle="tab" href="#form" aria-controls="form" aria-selected="true">Formulario</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" id="multimedia-tab" data-toggle="tab" href="#multimedia" aria-controls="multimedia" aria-selected="false">Multimedia</a>
  </li>
</ul>
<div class="tab-content">
  <div class="tab-pane fade show active" id="form" role="tabpanel" aria-labelledby="form-tab">
    <div class="col-12">
      <%= form_for [:admin, @article], html: {multipart: true} do |form| %>
      <%= render partial: 'form', locals:{f: form} %>
      <%= form.submit %>
      <% end %>
    </div>
  </div>
  <div class="tab-pane fade" id="multimedia" role="tabpanel" aria-labelledby="multimedia-tab">
    <div class="col-12">
      <%= render partial: "spree/multimedia/widgets/form_multimedia_upload" %>
    </div>
  </div>
</div>
